<% layout("../layouts/boilerplate") %>
<style>
body {
    font-family: 'Poppins', sans-serif;
    background-color: #f8f9fa;
    margin: 0;
}

/* Hero Section */
.hero {
    background: linear-gradient(rgba(0,0,0,0.5), rgba(0,0,0,0.5)),
                url('data:image/jpeg;base64,') no-repeat center center/cover;
    height: 70vh;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #fff;
    text-align: center;
    padding: 0 20px;
}

.hero h1 {
    font-size: 3rem;
    font-weight: bold;
    margin-bottom: 15px;
}

.hero p {
    font-size: 1.2rem;
    margin-bottom: 25px;
}

.hero .btn {
    font-size: 1.1rem;
    padding: 12px 30px;
    border-radius: 50px;
    transition: 0.3s;
}

.hero .btn:hover {
    transform: scale(1.05);
}

/* Popular Routes Section */
#buses h2 {
    font-weight: 600;
    color: #0d6efd;
    margin-bottom: 30px;
}

.bus-card {
    background: #fff;
    border-radius: 15px;
    overflow: hidden;
    box-shadow: 0 6px 15px rgba(0,0,0,0.1);
    transition: transform 0.3s;
}

.bus-card:hover {
    transform: translateY(-5px);
}

.bus-card img {
    width: 100%;
    height: 180px;
    object-fit: cover;
}

.bus-card-body {
    padding: 15px;
}

.bus-card-body h5 {
    font-size: 1.1rem;
    font-weight: 600;
    margin-bottom: 10px;
}

.bus-card-body p {
    font-size: 0.95rem;
    color: #555;
    margin-bottom: 10px;
}

.book-btn {
    width: 100%;
    padding: 10px;
    background-color: #198754;
    border: none;
    color: #fff;
    border-radius: 10px;
    font-weight: 500;
    transition: 0.3s;
}

.book-btn:hover {
    background-color: #157347;
}

/* Footer */
footer {
    background-color: #0d6efd;
    color: #fff;
    padding: 15px 0;
    font-size: 0.95rem;
}
</style>

<body>
  <!-- Hero Section -->
  <section class="hero">
    <div>
      <h1>Welcome to Safar.com</h1>
      <p>Book your bus tickets quickly, safely, and comfortably</p>
      <a href="/home" class="btn btn-light">Book Tickets</a>
    </div>
  </section>

  <!-- Popular Bus Routes Section -->
  <section id="buses" class="container my-5">
    <h2 class="text-center">Popular Routes</h2>
    <div class="row row-cols-1 row-cols-md-3 g-4">
      <!-- Example Card 1 -->
      <div class="col">
        <div class="bus-card">
          <div class="bus-card-body">
            <h5>Channei → Madhurai</h5>
            <p>Fast and comfortable AC buses available</p>
            <a href="/show/68d7699992391ca76f153e04" class="btn book-btn">Book Now</a>
          </div>
        </div>
      </div>

      <!-- Example Card 2 -->
      <div class="col">
        <div class="bus-card">
          <div class="bus-card-body">
            <h5>Rachi→ Patna</h5>
            <p>Enjoy a safe journey with premium buses</p>
            <a href="/show/68d7699992391ca76f153e0b" class="btn book-btn">Book Now</a>
          </div>
        </div>
      </div>

      <!-- Example Card 3 -->
      <div class="col">
        <div class="bus-card">
          <div class="bus-card-body">
            <h5>Ahemdabad → Surat</h5>
            <p>Comfortable journeys with onboard amenities</p>
            <a href="/show/68d7699992391ca76f153e05" class="btn book-btn">Book Now</a>
          </div>
        </div>
      </div>


       <div class="col">
        <div class="bus-card">
          <div class="bus-card-body">
            <h5>Solapur → Indore</h5>
            <p>Comfortable journeys with premium experience</p>
            <a href="/show/68d553a55667b9d91b97e45e" class="btn book-btn">Book Now</a>
          </div>
        </div>
      </div>


       <div class="col">
        <div class="bus-card">
          <div class="bus-card-body">
            <h5>Deheradun → Haridwar</h5>
            <p>Ac slepper with Wifi and Drink</p>
            <a href="/show/68d7699992391ca76f153e07" class="btn book-btn">Book Now</a>
          </div>
        </div>
      </div>



       <div class="col">
        <div class="bus-card">
          <div class="bus-card-body">
            <h5>Nashik → Nagpur</h5>
            <p>Fast and comfortable AC buses available</p>
            <a href="/show/68d565baadac6ce8f3dbc215" class="btn book-btn">Book Now</a>
          </div>
        </div>
      </div>




    </div>
  </section>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
</body>
